            <artifactId>jboss-j2ee</artifactId>
            <version>3.2.1</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-full</artifactId>
            <version>1.0.1</version>
